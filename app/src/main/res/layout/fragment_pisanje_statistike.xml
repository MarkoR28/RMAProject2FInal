<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="rs.raf.projekat2.marko_radojevic_rn7417.presentation.view.fragments.PisanjeStatistikeFragment">

<rs.raf.projekat2.marko_radojevic_rn7417.presentation.view.custom.SquareView
    android:id="@+id/squareView"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:rotation="180"
    app:layout_constraintBottom_toTopOf="@+id/applyBarHeightBtn"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    />

<Button
    android:id="@+id/applyBarHeightBtn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="50dp"
    android:text="Apply bar height"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>